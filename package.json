{
  "name": "k6-load-test",
  "version": "1.0.0",
  "description": "K6 Load Testing with InfluxDB and Grafana",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "logs": "docker-compose logs -f",
    "logs:grafana": "docker-compose logs -f grafana",
    "logs:influxdb": "docker-compose logs -f influxdb",
    "test:load": "docker-compose exec k6 k6 run test.ts --out influxdb=http://influxdb:8086/k6",
    "rebuild": "docker-compose down -v && docker-compose up -d",
    "check-influx": "docker-compose exec influxdb influx -execute 'SHOW DATABASES'",
    "check-grafana": "curl -u admin:admin http://localhost:3033/api/health",
    "compile": "tsc"
  },
  "keywords": ["k6", "load-testing", "performance", "influxdb", "grafana"],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/k6": "^1.1.1"
  },
  "dependencies": {
    "typescript": "^5.9.2"
  }
}
